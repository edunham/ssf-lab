{
  "mcpServers": {
    "ssf-security": {
      "command": "python",
      "args": ["/workspace/mcp-server/server.py"],
      "cwd": "/workspace/mcp-server"
    }
  },
  "python.defaultInterpreterPath": "/usr/local/bin/python",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  }
}